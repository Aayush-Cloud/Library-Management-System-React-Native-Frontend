{
  "expo": {
    "name": "prac",
    "slug": "prac",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#e63c4b"
    },
    
  
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.aayush.libman",
      "versionCode": 1
    },
    "web": {
      "favicon": "./app/assets/icon.png"
    },
    "notification": {
      "iosDisplayInForeground": true}
  }
}
